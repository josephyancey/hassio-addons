{
	"name": "Static File Server",
	"version": "1.1.4",
	"slug": "staticserver",
	"description": "Just serves static files",
	"startup": "before",
	"boot": "auto",
	"map": ["config:rw"],
	"ports": {
		"8000/tcp": 8000
	},
	"options": {
		"pathtohost": "/config/static"
	},
	"schema": {
		"pathtohost": "str"
	}
}
